import datetime
from pydantic import BaseModel 


# Pydantic model for data validation
class Message(BaseModel):
    id: int
    user_id: int
    name: str
    is_bot: bool
    is_respose: bool
    is_email: bool
    message: str
    timestamp: datetime
    
class User(BaseModel):
    id: int
    email: str
    username: str
    password: str
    name: str